# Production Environment Variables
# Copy this file to .env.production and fill in the actual values

# Server Configuration
PORT=3000
NODE_ENV=production

# Application URLs
OPENPHONE_WEBHOOK_URL=http://your-coolify-domain.com/webhooks/openphone
